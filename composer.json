{
    "name": "kuri0616/gh-analyzer",
    "description": "GitHub Analysis CLI Application",
    "keywords": ["github", "analysis", "cli"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.2",
        "symfony/console": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "gh-analyzer"
    ]
}