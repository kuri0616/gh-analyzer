{
    "name": "kuri0616/gh-analyzer",
    "description": "GitHub repository analyzer CLI application",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "laravel-zero/framework": "^11.45",
        "guzzlehttp/guzzle": "^7.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "builds/gh-analyzer"
    ],
    "scripts": {
        "test": "phpunit"
    }
}
